<div class="profile-header">
  <div class="thumbnail">
    <%= cl_image_tag current_user.image, class: "avatar-profile" %>
  </div>
    <h1 class="h1-profile-wm">Welcome <%= current_user.first_name %>!</h1>
    <% if current_user.money_level.nil? %>
      <%= link_to "Finish registration", edit_user_path(current_user) %>
    <% elsif current_user.user_interests.empty? %>
    <%= link_to "Fill in your interests", new_user_interest_path(current_user) %>
      <% end %>
  <%= image_tag 'setting-dark.png', class: "setting dropdown-toggle", id: "settingDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
  <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
    <%= link_to "Edit Preferences", edit_user_path(current_user), class: "dropdown-item" %>
    <%= link_to "Edit Profile", edit_user_registration_path, class: "dropdown-item" %>
    <%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
  </div>
</div>

